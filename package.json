{
  "name": "wisite-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.0.0",
    "axios": "^0.26.0",
    "bootstrap": "^5.1.3",
    "bootstrap-vue": "^2.21.2",
    "child_process": "^1.0.2",
    "core-js": "^3.21.1",
    "element-plus": "^2.0.4",
    "eslint-loader": "^4.0.2",
    "express": "^4.17.3",
    "http": "0.0.1-security",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "vue": "^3.2.31",
    "vue-i18n": "^9.1.9",
    "vue-loader-v16": "^16.0.0-beta.5.4",
    "vue-router": "^4.0.13",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.31",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.20.0",
    "vue-picture-cropper": "^0.5.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-debugger": 0,
      "no-console": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bin": "./service.js",
  "pkg": {
    "assets": [
      "./dist/**/*"
    ],
    "targets": [
      "node10-linux-x64",
      "node10-win-x64"
    ],
    "output": "wisite-spa"
  }
}
